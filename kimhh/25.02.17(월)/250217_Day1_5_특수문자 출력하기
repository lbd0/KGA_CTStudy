#include <iostream>

using namespace std;

int main(void) {
    cout << (char)33;
    cout << (char)64;
    cout << (char)35;
    cout << (char)36;
    cout << (char)37;
    cout << (char)94;
    cout << (char)38;
    cout << (char)42;
    cout << (char)40;
    cout << (char)92;
    cout << (char)39;
    cout << (char)34;
    cout << (char)60;
    cout << (char)62;
    cout << (char)63;
    cout << (char)58;
    cout << (char)59;
    return 0;
}

